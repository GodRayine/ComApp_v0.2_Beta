<Window x:Class="ComApp_v0._2_Beta.AppWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComApp_v0._2_Beta"
        mc:Ignorable="d"
        Title="AppWindow" Height="300" Width="300" WindowStyle="None" ResizeMode="NoResize" Background="#FF00FF33">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid Name="ButtonContainer" Grid.Row="1">
            <Button Name="Button2" Height="30" Width="120"
                    Content="Обратно" Background="#FF330606" BorderBrush="{x:Null}" Foreground="White" Click="Button2_Click" />
        </Grid>
    </Grid>
</Window>
